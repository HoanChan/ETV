{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Test Train MMOCR",
      "type": "shell",
      "command": "conda",
      "args": [
        "run",
        "-n",
        "etv",
        "python",
        "tools/train.py",
        "src/configs/etv_concat_resnetex_ranger.py",
        "--work-dir",
        "work_dirs/etv_concat_resnetex_ranger",
        "--cfg-options",
        "train_cfg.max_epochs=1",
        "train_dataloader.dataset.max_data=1000",
        "val_cfg=None",
        "val_dataloader=None",
        "val_evaluator=None",
        "test_cfg=None",
        "test_dataloader=None",
        "test_evaluator=None",
        "default_hooks.checkpoint=None",
      ],
      "options": {
        "env": {
          "PYTHONPATH": "${workspaceFolder}/src"
        },
      },
      "group": {
        "kind": "build"
      }
    }
  ]
}
